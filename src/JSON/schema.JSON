          "objPrn": {
            "title": "Object Pronoun",
            "type": "string"
          },
          "possAdj": {
            "title": "Possesive Adjective",
            "type": "string"
          },
          "possPrn": {
            "title": "Possesive Pronoun",
            "type": "string"
          },
          "rflxPrn": {
            "title": "Reflexive Pronoun",
            "type": "string"
          }
        },
        "required": [
          "key",
          "name",
          "subPrn",
          "objPrn",
          "possAdj",
          "possPrn",
          "rflxPrn"
        ]
      },
      "ideal": {
        "title": "Ideal",
        "description": "A two part ideal. A Word, and Phrase.",
        "type": "object",
        "properties": {
          "name": {
            "title": "Ideal Name",
            "description": "The Name of the Ideal.",
            "type": "string"
          },
          "description": {
            "title": "description",
            "description": "A brief description of the ideal.",
            "type": "string"
          }
        }
      }
    }
  },
